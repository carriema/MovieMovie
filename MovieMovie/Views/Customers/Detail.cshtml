@model MovieMovie.Models.Customer

@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li>Membership Type: @Model.Membership.MembershipType</li>
    @if (Model.Birthday.HasValue)
    {
        <li>Birthdate: @Model.Birthday.Value.ToShortDateString()</li>
    }
</ul>

